package learningJava;

public class Main {

    public static void main(String[] args) {
        String[] arr = {"Charizard", "Mew", "Articuno", "Blastoise", "Venasaur"};

        int count = 0;
        for (String element: arr) {
            if (element.equals("Blastoise")) {
                System.out.println("We have found Blastoise, breaking.");
                break;
            } else {
                System.out.println(element + " is index " + count);
                count++;
            }

        }

        int[] nums = new int[5];
        for (int i = 0; i < nums.length; i++) {
            nums[i] += i;
        }
        for (int el: nums) {
            System.out.print(el);
        }
    }
}
